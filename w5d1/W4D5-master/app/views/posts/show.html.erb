<div class="container">


Title: <%= @post.title %><br>
URL: <%= @post.url  %><br>
Content: <%= @post.content  %><br>
Comments: <% @post.comments.each do |comment| %>
          <li>  <%= comment.content %></li>
          <% end %>


<%= link_to "Comment on this post!", new_post_comment_url(@post), class: "btn btn-lg btn-success" %>
</div>
